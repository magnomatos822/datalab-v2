# -- Credenciais Globais --
POSTGRES_USER=admin
POSTGRES_PASSWORD=supersecretpassword

MINIO_ROOT_USER=admin
MINIO_ROOT_PASSWORD=supersecretpassword

# -- Configurações do Prefect --
PREFECT_API_URL=http://prefect-server:4200/api

# -- Configurações do MLflow --
# As credenciais AWS são herdadas de MINIO_ROOT_USER/PASSWORD no docker-compose
MLFLOW_S3_ENDPOINT_URL=http://minio:9000

# -- Configurações do Apache Hop --
HOP_PASSWORD=supersecretpassword
S3_BRONZE_BUCKET=bronze
S3_SILVER_BUCKET=silver
S3_GOLD_BUCKET=gold

# -- Configurações do Snowflake --
# Para produção, use valores reais da sua conta Snowflake
SNOWFLAKE_ACCOUNT=your-account.snowflakecomputing.com
SNOWFLAKE_USER=your-snowflake-user
SNOWFLAKE_PASSWORD=your-snowflake-password
SNOWFLAKE_ROLE=TRANSFORMER
SNOWFLAKE_WAREHOUSE=COMPUTE_WH
SNOWFLAKE_DATABASE=DATALAB_DB
SNOWFLAKE_SCHEMA=ANALYTICS

# -- Configurações do Apache Spark --
# Recursos para cada nó worker do Spark.
SPARK_WORKER_CORES=1
SPARK_WORKER_MEMORY=2G

# -- Configurações DBT --
DBT_ENVIRONMENT=development
DBT_TARGET=snowflake_dev